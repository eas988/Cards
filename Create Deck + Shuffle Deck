def create_deck():
  Cards = ["Jack", "Queen", "King", "Ace"]
  Cards += list(range(2, 10))
  Suits = ["Spades", "Hearts", "Diamonds", "Clubs"]
  Deck = []
  for Card in Cards:
    for Suit in Suits:
      Deck += [str(Card)+" of "+str(Suit)]
  return Deck

def shuffle_deck():
  Fresh_Deck = create_deck()
  random.shuffle(Fresh_Deck)
  return Fresh_Deck

print(shuffle_deck())
